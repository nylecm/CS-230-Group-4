<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="546.0" minWidth="720.0" prefHeight="546.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogInController">
    <children>
      <VBox alignment="CENTER" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" spacing="20.0">
         <children>
              <Label text="Login">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
              <TextField fx:id="username" maxHeight="400.0" maxWidth="400.0" minWidth="70.0" promptText="Username">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
              <TextField fx:id="password" maxHeight="400.0" maxWidth="400.0" minWidth="70.0" promptText="Password" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <HBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="200.0" prefHeight="63.0" prefWidth="398.0" spacing="20.0">
               <children>
                  <Label alignment="CENTER" text="Forgot password?" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets top="7.0" />
                     </HBox.margin>
                  </Label>
                  <Button fx:id="logInButton" mnemonicParsing="false" onDragDetected="#onLoginButtonClicked" prefHeight="25.0" prefWidth="62.0" text="Login" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
      <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="400.0" minWidth="200.0" prefHeight="71.0" prefWidth="400.0" text="Don't have an account yet?" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button minWidth="100.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="100.0" text="Register" />
      <Button minWidth="50.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="400.0" text="Back">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </Button>
    </children>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>
</VBox>
